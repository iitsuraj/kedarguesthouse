<% layout('panel') -%>
<div class="row justify-content-end">
    <div class="col-md-3">
        <a href="/panel/subscriber/all.csv" class="btn btn-primary">All Subscriber</a>
    </div>
</div>
<div class="row justify-content-center">
    <div class="col-md-10">
        <div class="card">
            <div class="card-header card-header-primary">
                <h4 class="card-title ">Latest Email Subscribers</h4>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead class=" text-primary">
                            <th>
                                ID
                            </th>
                            <th>
                                Email
                            </th>
                        </thead>
                        <tbody>
                            <%for(var i =0; i < subscriber.length; i++){%>
                            <tr>
                                <td>
                                    <%= i+1%>
                                </td>
                                <td>
                                    <%= subscriber[i].email%>
                                </td>
                            </tr>
                            <%}%>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>